func TestPart1(t *testing.T) {

	// Test part 1
	puzzle := `<puzzle>`
	answer := `<answer>`

	// Solve
	result := part1(utils.ParseString(puzzle))

	if result != answer {
		t.Fatalf("Failed!: %v != %v", answer, result)
	}
	fmt.Println(fmt.Sprintf("Part 1 [%s]: %v == %v", utils.FormatGreen("solved"), answer, result))
}

